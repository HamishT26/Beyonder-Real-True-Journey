{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Freed ID Minimum Disclosure Presentation v0",
  "type": "object",
  "required": [
    "subject_did",
    "credential_id",
    "requested_fields",
    "disclosed_claims",
    "redacted_fields",
    "denied_sensitive_fields",
    "policy_version",
    "created_utc"
  ],
  "properties": {
    "subject_did": {
      "type": "string"
    },
    "credential_id": {
      "type": "string"
    },
    "requested_fields": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "disclosed_claims": {
      "type": "object",
      "additionalProperties": true
    },
    "redacted_fields": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "denied_sensitive_fields": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "policy_version": {
      "type": "string"
    },
    "created_utc": {
      "type": "string"
    }
  },
  "additionalProperties": true
}
